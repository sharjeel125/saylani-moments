{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/register/page.tsx", "app/layout.tsx", "app/page.tsx", "app/matched-images/page.tsx", "app/matched-images/loading.tsx", "components/header.tsx", "components/image-modal.tsx", "components/partner-scroll.jsx", "components/selfie-capture.tsx", "components/theme-provider.tsx", "lib/utils.ts", "lib/firebase.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
